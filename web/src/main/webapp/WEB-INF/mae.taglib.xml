<?xml version="1.0"?>
<facelet-taglib version="2.0"
	xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facelettaglibrary_2_0.xsd">

	<namespace>http://ec.gob.ambiente/mae-components</namespace>
	<tag>
		<tag-name>readOnly</tag-name>
		<component>
			<component-type>ec.gob.ambiente.core.components.ReadOnly</component-type>
			<renderer-type>ec.gob.ambiente.core.components.ReadOnly</renderer-type>
		</component>
		<attribute>
			<name>id</name>
			<required>false</required>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<name>value</name>
			<required>false</required>
			<type>java.lang.Boolean</type>
		</attribute>
		<attribute>
			<name>rendered</name>
			<required>false</required>
			<type>java.lang.Boolean</type>
		</attribute>
	</tag>
</facelet-taglib>